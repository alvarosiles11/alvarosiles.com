{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-tabs": "off",
    "no-console": "off",
    "no-plusplus": "off",
    // "none - Import": "off",
    "sort-imports": [
      "error",
      {
        "ignoreCase": false,
        "ignoreDeclarationSort": false,
        "ignoreMemberSort": false,
        "memberSyntaxSortOrder": ["none", "all", "multiple", "single"],
        "allowSeparatedGroups": false
      }
    ]
    //     "semi": "error",
    //     "no-unused-vars": "off",
    //     "func-names": "off",
    //     "indent": "off",
    //     "no-else-return": "off",
    //     "prefer-arrow-callback": "off",
    //     "no-undef": "off",
    //     "no-use-before-define": "off",
    //     "comma-dangle": "off",
    //     "eol-last": "off",
    //     "no-trailing-spaces": "off",
    //     "linebreak-style": "off",
    //     "no-console": "off",
    //     "no-restricted-globals": "off",
    //     "object-shorthand": "off",
    //     "no-shadow": "off",
    //     "no-debugger": "off",
    //     "prefer-const": "off",
    //     "no-multiple-empty-lines": "off"
  }
}
